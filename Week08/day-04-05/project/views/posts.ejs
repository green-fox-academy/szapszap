       <% rows.forEach( (row) => { %>
        <li><%= (`${row.title}: ${row.content}' posted by 
          ${row.user} at ${row.date_posted.toString().substring(0, 15)}`); %>
    <% }); %></li>
    </ul>
    <table>
      <form>
      </form>
    </table>
    
  </h2>
  </article>
  </body>
</html>